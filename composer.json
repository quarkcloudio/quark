{
    "name": "quarkcms/quark",
    "description": "quark core lib",
    "keywords": [
        "quark",
        "quark-admin",
        "sdk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "tangtanglove",
            "email": "dai_hang_love@126.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "QuarkCMS\\Quark\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
